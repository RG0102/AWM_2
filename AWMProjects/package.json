{
  "name": "frontend-spa",
  "version": "1.0.0",
  "description": "A simple frontend SPA with Webpack and npm",
  "private": true,
  "scripts": {
    "start-dev": "webpack --mode=development --config webpack.config.js",
    "start-prod": "webpack --mode=production --config webpack.config.js",
