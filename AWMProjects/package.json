    "html-webpack-plugin": "^5.5.0",
    "http-server": "^0.12.3",
    "mini-css-extract-plugin": "^2.7.0",
    "node-sass": "^6.0.0",
    "offline-plugin": "^5.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "sass-loader": "^12.0.0",
    "serviceworker-webpack-plugin": "^1.0.1",
    "style-loader": "^3.3.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
